# ANUS AI Framework Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI API Key - Required for using OpenAI models
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key - Required only if using Claude models
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Other configuration options
ANUS_ENV=development
ANUS_LOG_LEVEL=INFO 